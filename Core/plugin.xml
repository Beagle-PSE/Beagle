<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="de.uka.ipd.sdg.beagle.measurementtool" name="MeasurementTool" schema="schema/de.uka.ipd.sdg.beagle.measurementtool.exsd"/>

   <extension
         point="org.eclipse.ui.commands">
      <category
            name="Beagle"
            id="de.uka.ipd.sdq.beagle.extensionpointprototype.commands.categoryBeagle">
      </category>
      <command
            name="Available Measurment Tools"
            categoryId="de.uka.ipd.sdq.beagle.extensionpointprototype.commands.categoryBeagle"
            id="de.uka.ipd.sdq.beagle.extensionpointprototype.commands.showMeasurmentTools">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="de.uka.ipd.sdq.beagle.extensionpointprototype.commands.showMeasurmentTools"
            class="de.uka.ipd.sdq.beagle.prototypes.extensionpoint.ShowMeasurementToolsHandler">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="de.uka.ipd.sdq.beagle.extensionpointprototype.commands.showMeasurmentTools"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>
</plugin>
